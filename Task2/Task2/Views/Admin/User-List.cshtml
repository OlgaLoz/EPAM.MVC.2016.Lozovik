@model IEnumerable<Task2.Models.User>

@{
    ViewBag.Title = "UserList";
}

<h2>UserList</h2>

<table class="table">
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(x => item.Name)
            </td>
            <td>
                @Html.ActionLink("delete", "DeleteUser", new {id = item.Id})
            </td>
        </tr>
    }
</table>